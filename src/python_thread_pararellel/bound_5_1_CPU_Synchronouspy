'''
    Section 3
    Concurrency, CPU Bound, I/O Bound - CPU bound Sync
    Keyword = CPU Bound
    
'''

# 요청을 최소한으로 하며, 최대한 CPU만을 사용해 계산하는 것

import time

def cpu_bound(n):
    return sum(i * i for i in range(n))

def find_sums(n) -> list:
    result = []
    for num in n:
        result.append(cpu_bound(num))
    return result

def main():
    numbers = [3_000_000 + x for x in range(30)]
    # 실행 시간 측정
    st_ = time.time()
    total = find_sums(numbers)
    ed_ = time.time()
    print()
    print(f"Total : {total}")
    print(f'sum : {sum(total)}')
    duration = ed_ - st_
    print(f"Dutarion : {duration}s")

if __name__ == "__main__":
    main()